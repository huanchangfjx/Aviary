<resources xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aviary="http://schemas.android.com/apk/res-auto">

    <style name="AviaryWidget"></style>

    <style name="AviaryWidget.AviarySeekBar" parent="@android:style/Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/aviary_seekbar_background</item>
        <item name="aviarySeekBarSecondary">@drawable/aviary_seekbar_secondary_selector</item>
        <item name="aviarySeekBarSecondaryInverted">@drawable/aviary_seekbar_secondary_inverted_selector</item>
        <item name="aviarySeekBarSecondaryCenter">@drawable/aviary_seekbar_secondary_center_selector</item>
        <item name="aviarySeekBarThumb">@drawable/aviary_seekbar_thumb</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:minHeight">46dip</item>
        <item name="android:maxHeight">46dip</item>
        <item name="aviarySeekBarThumbOffset">16dip</item>
        <item name="android:paddingLeft">32dip</item>
        <item name="android:paddingRight">32dip</item>
    </style>

    <style name="AviaryWidget.AviaryWheel">
        <item name="aviaryWheelShadowTop">@drawable/aviary_wheel_shadow_bottom</item>
        <item name="aviaryWheelLine">@drawable/aviary_wheel_line</item>
        <item name="aviaryWheelIndicator">@drawable/aviary_wheel_indicator</item>
        <item name="android:background">@drawable/aviary_wheel_background</item>
        <item name="android:layout_marginLeft">@dimen/aviary_wheel_margin_horizontal</item>
        <item name="android:layout_marginRight">@dimen/aviary_wheel_margin_horizontal</item>
        <item name="aviary_edgeStyle">?aviaryEdgeEffectDefaultStyle</item>
    </style>

    <style name="AviaryWidget.TiltShiftView">
        <item name="aviary_strokeColor">#AAFFFFFF</item>
        <item name="aviary_strokeWidth">3dip</item>
        <item name="aviary_shape_minsize">10</item>
        <item name="aviary_shape_maxsize">95</item>
        <item name="aviary_shape_defaultsize">100dip</item>
        <item name="aviary_crosshair_strokeWidth">2</item>
        <item name="aviary_crosshair_radius">6dip</item>
        <item name="aviary_crosshair_edge">3dip</item>
        <item name="aviary_strokeColor2">#AA000000</item>
        <item name="aviary_timeout">2000</item>
        <item name="aviary_animationDuration">200</item>
        <item name="aviaryWave_pointDrawable">@drawable/aviary_wave_dot</item>
        <item name="aviaryWave_innerRadius">15dip</item>
        <item name="aviaryWave_outerRadius">135dip</item>
        <item name="aviaryWave_animationDuration">2000</item>
        <item name="aviaryCrosshair_enabled">0</item>
    </style>

    <style name="AviaryWidget.SymbolPlus">
        <item name="android:scaleType">centerInside</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:background">@drawable/aviary_symbol_plus_normal</item>
    </style>

    <style name="AviaryWidget.SymbolMinus">
        <item name="android:scaleType">centerInside</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:background">@drawable/aviary_symbol_minus_normal</item>
    </style>

    <style name="AviaryWidget.BottomBar"></style>

    <style name="AviaryWidget.BottomBar.GalleryItem"></style>

    <style name="AviaryWidget.BottomBar.GalleryItem.Container">
        <item name="android:clickable">false</item>
        <item name="android:layout_width">@dimen/aviary_gallery_item_width</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/aviary_gallery_default_background_selector</item>
    </style>

    <style name="AviaryWidget.BottomBar.GalleryItem.Divider">
        <item name="android:layout_width">@dimen/aviary_nav_divider_double</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/aviary_bottombar_vline_selector</item>
    </style>

    <style name="AviaryWidget.BottomBar.GalleryItem.HighlightImageView" parent="AviaryWidget.HighlightImageView">
        <item name="aviary_glowMode">checked|selected</item>
        <item name="aviary_highlightMode">pressed|checked|selected</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="aviary_toggleable">true</item>
    </style>

    <style name="AviaryWidget.BottomBar.GalleryItem.Crop"></style>

    <style name="AviaryWidget.BottomBar.GalleryItem.Crop.TextView" parent="AviaryWidget.TextView">
        <item name="android:textAppearance">?aviaryTextAppearanceMedium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:shadowColor">@android:color/black</item>
        <item name="android:shadowDx">2.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">2.0</item>
        <item name="aviary_typeface">?attr/aviaryFontRegular</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="AviaryWidget.BottomBar.GalleryItem.Crop.TextView.Custom">
        <item name="android:textSize">@dimen/aviary_textSizeMedium</item>
        <item name="aviary_typeface">?aviaryFontLight</item>
    </style>

    <style name="AviaryWidget.BottomBar.GalleryItem.Crop.HighlightImageView" parent="AviaryWidget.HighlightImageView">
        <item name="android:adjustViewBounds">true</item>
        <item name="android:clickable">false</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:src">@drawable/aviary_crop_invert_indicator_selector</item>
        <item name="aviary_toggleable">true</item>
        <item name="aviary_highlightMode">checked</item>
        <item name="aviary_glowMode">checked</item>
    </style>

    <style name="AviaryWidget.BottomBar.GalleryTopIndicatorView">
        <item name="android:layout_height">@dimen/aviary_gallery_top_indicator_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="aviary_drawableStyle">?aviaryOptionPanelTopIndicatorStyle</item>
    </style>

    <style name="AviaryWidget.BottomBar.GalleryTopIndicatorView.Inverse">
        <item name="aviary_drawableStyle">?aviaryOptionPanelBottomIndicatorStyle</item>
    </style>

    <style name="AviaryWidget.BottomBar.ViewFlipper">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?aviaryBottomBarHeight</item>
        <item name="android:animationCache">true</item>
        <item name="android:autoStart">false</item>
        <item name="android:persistentDrawingCache">animation</item>
        <item name="android:inAnimation">@anim/aviary_bottobar_in_animation</item>
        <item name="android:outAnimation">@anim/aviary_bottobar_out_animation</item>
    </style>

    <style name="AviaryWidget.BottomBar.Logo"></style>

    <style name="AviaryWidget.BottomBar.Logo.Container">
        <item name="android:background">@drawable/aviary_bottombar_logo_background</item>
        <item name="android:gravity">center_vertical|right</item>
        <item name="android:paddingBottom">2dip</item>
        <item name="android:paddingLeft">8dip</item>
        <item name="android:paddingRight">8dip</item>
        <item name="android:paddingTop">4dip</item>
    </style>

    <style name="AviaryWidget.BottomBar.Logo.Image">
        <item name="android:adjustViewBounds">true</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:src">@drawable/aviary_powered</item>
    </style>

    <style name="AviaryWidget.BottomBar.OptionPanel">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?aviaryBottomBarHeight</item>
        <item name="android:background">@drawable/aviary_bottombar_tools_background</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">0dip</item>
        <item name="android:paddingRight">0dip</item>
        <item name="android:paddingBottom">0dip</item>
        <item name="android:paddingTop">0dip</item>
    </style>
    
    <style name="AviaryWidget.BottomBar.OptionPanelContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">8dip</item>
        <item name="android:paddingRight">8dip</item>
        <item name="android:paddingBottom">6dip</item>
        <item name="android:paddingTop">6dip</item>
    </style>
    
    <style name="AviaryWidget.BottomBar.OptionPanelContainer.BorderLess">
        <item name="android:paddingLeft">0dip</item>
        <item name="android:paddingRight">0dip</item>
        <item name="android:paddingBottom">0dip</item>
        <item name="android:paddingTop">0dip</item>        
    </style>

    <style name="AviaryWidget.BottomBar.HorizontalScrollView">
        <item name="android:background">@drawable/aviary_bottombar_tools_background</item>
        <item name="android:paddingTop">0dip</item>
        <item name="android:paddingBottom">0dip</item>
        
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:divider">@null</item>
        <item name="dividerWidth">0dip</item>
        <item name="stackFromRight">false</item>
        <item name="android:smoothScrollbar">false</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:choiceMode">none</item>
        <item name="android:overScrollMode">always</item>        
    </style>
    
    <style name="AviaryWidget.EffectsHorizontalScrollView">
        <item name="android:background">@null</item>
        <item name="android:persistentDrawingCache">animation</item>
        <item name="android:scrollbars">none</item>
        <item name="android:paddingTop">0dip</item>
        <item name="android:paddingBottom">0dip</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:smoothScrollbar">false</item>
        <item name="android:divider">@null</item>
        <item name="dividerWidth">0dip</item>
        <item name="android:overScrollMode">always</item>
        <item name="android:choiceMode">singleChoice</item>
    </style>
    
    <style name="AviaryWidget.EffectThumb"></style>
    
	<style name="AviaryWidget.EffectThumb.Divider">
        <item name="android:layout_width">@dimen/aviary_nav_divider_double</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/aviary_bottombar_thumb_divider_right</item>
    </style>
    
	<style name="AviaryWidget.EffectThumb.DividerContainer">
	    <item name="android:background">@drawable/aviary_bottombar_thumb_external_background</item>
	    <item name="android:paddingLeft">6dip</item>
	    <item name="android:paddingRight">0dip</item>
	</style>
	
	<style name="AviaryWidget.EffectThumb.DividerContainer.Left">
	    <item name="android:paddingLeft">0dip</item>
	    <item name="android:paddingRight">6dip</item>	    
	</style>
    
	<style name="AviaryWidget.EffectThumb.Divider.Left">
        <item name="android:layout_width">@dimen/aviary_nav_divider_double</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/aviary_bottombar_thumb_divider_left</item>        
    </style>
    
	<style name="AviaryWidget.EffectThumb.PacksDivider">
	    <!-- <item name="android:src">@drawable/aviary_frame_divider_background</item> -->
	    <item name="android:background">@drawable/aviary_frame_divider_background</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    
    <style name="AviaryWidget.EffectThumb.Text">
        <item name="android:textAppearance">?aviaryTextAppearanceSmallNoSelection</item>
        <item name="android:shadowColor">@android:color/black</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">2.0</item>
        <item name="aviary_typeface">?attr/aviaryFontRegular</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLines">1</item>
        <item name="android:layout_marginBottom">@dimen/aviary_effect_thumb_margin_bottom</item>
        <item name="android:layout_marginTop">0dip</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">0dip</item>
    </style>
    
    <style name="AviaryWidget.EffectThumb.HiddenSelector">
        <item name="android:paddingBottom">0dip</item>
        <item name="android:paddingTop">0dip</item>
        <item name="android:src">@drawable/aviary_frame_item_selected_image</item>
    </style>
    
    <style name="AviaryWidget.EffectThumb.Layout">
        <item name="aviary_animationDuration">100</item>
    </style>
    
    <style name="AviaryWidget.EffectThumb.Layout.External">
        <item name="android:background">@drawable/aviary_bottombar_thumb_external_background</item>
    </style>
    

